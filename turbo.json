{
  "$schema": "https://turbo.build/schema.json",
  "pipeline": {
    "build": {
      "outputs": [".next/**", "!.next/cache/**"]
    },
    "lint": {},
    "open-next:build": {
      "cache": true,
      "dependsOn": ["build"],
      "outputs": [".open-next/**"]
    },
    "create-artifacts": {
      "cache": true,
      "dependsOn": ["open-next:build"],
      "outputs": ["artifacts/**"]
    }
  }
}
